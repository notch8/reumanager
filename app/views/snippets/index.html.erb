<h1>Set up your account</h1>
<%= form_for(@grant, url: snippets_path, method: :post) do |f| %>
  <%= f.fields_for :snippets do |snippets_f| %>
    <%= snippets_f.label :value, snippets_f.object.name %>
    <%= snippets_f.text_field :value %>
    <div class='hint'><%= snippets_f.object.description %></div>
  <% end %>

  <%= f.submit :update %>
<% end %>

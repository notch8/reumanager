= render "layouts/navbar"

%header.container
  .header-inner.main-header
    %h1{ :id => "header_link_1" }
      #{link_to(Setting[:university], Setting[:university_url])}, #{link_to(Setting[:department], Setting[:department_url])}
    %h2{ :id => "header_link_2" }
      = link_to("#{Setting[:app_title]} (#{Setting[:program_start_date].to_date.strftime("%A, %b %d")} - #{Setting[:program_end_date].to_date.strftime("%A, %b %d")})", Setting[:program_url])
